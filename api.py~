import requests
import json


request = requests.get('https://na1.api.riotgames.com/lol/platform/v3/champion-rotations?api_key=RGAPI-4c01dd81-4dc2-4ce8-9d0a-716b12124110')
requestT = request.text
requestJ = json.loads(requestT)
print(requestJ['freeChampionIds'])
freeChampionIDArr = requestJ['freeChampionIds']
with open("sample.json", "w") as outfile:
        outfile.write(requestJ) 
